{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "no-console": false,
        "ordered-imports": false,
        "max-classes-per-file": false,
        "quotemark": [
            true,
            "single"
        ],
        "file-name-casing": [
            true,
            "camel-case"
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "member-ordering": false
    },
    "rulesDirectory": []
}